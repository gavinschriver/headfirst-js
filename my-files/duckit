		  <script type="text/javascript">
		  	function Duck(sound) {
		  		this.sound = sound;
		  		this.quack = function() {console.log(this.sound);}
		  	}

		  	var toy = new Duck("quack quack");

		  	toy.quack();

		  	console.log(typeof toy);
		  	console.log(typeof instanceof Duck);
		  	
		  </script>
